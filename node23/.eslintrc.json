{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": ["error", 4],
        "linebreak-style": ["error", "windows"],
        "comma-dangle": ["error", "never"],
        "object-shorthand": ["error", "never"],
        "no-underscore-dangle": 0,
        "lines-between-class-members": 0
    }
}
